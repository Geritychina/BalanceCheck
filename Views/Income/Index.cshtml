@model IEnumerable<BalanceCheck.Models.Income>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IncomeValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsRepeated)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IncomeDate)
            </th>
             <th>
               
                @Html.DisplayNameFor(model => model.EndIncomeDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
       
            <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IncomeValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsRepeated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IncomeDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndIncomeDate)
            </td>
           
            
            <td>
                <a asp-action="Edit" asp-route-id="@item.IncomeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IncomeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IncomeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
